{
    "type": "js",
    "name": "SpankBang",
    "icon": "qdl2-spankbang.jpg",
    "version": 1,
    "regExp": "http(s|)://(m\\.|)spankbang\\.com/\\w+/(video|embed)",
    "settings": [
        {
            "type": "boolean",
            "label": "Use default video format",
            "key": "useDefaultVideoFormat",
